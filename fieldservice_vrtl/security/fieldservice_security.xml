<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="fieldservice_group_user" model="res.groups">
            <field name="name">Field Service User</field>
            <field name="category_id" ref="base.module_category_services_field_service"/>
        </record>

        <record id="fieldservice_group_manager" model="res.groups">
            <field name="name">Field Service Manager</field>
            <field name="category_id" ref="base.module_category_services_field_service"/>
            <field name="implied_ids" eval="[(4, ref('fieldservice_group_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>
