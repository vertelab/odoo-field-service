<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- New Stage -->
        <record id="fieldservice_stage_new" model="fieldservice.stage">
            <field name="name">New</field>
            <field name="sequence">10</field>
            <field name="fold">False</field>
            <field name="is_closed">False</field>
        </record>

        <!-- Ongoing Stage -->
        <record id="fieldservice_stage_ongoing" model="fieldservice.stage">
            <field name="name">Ongoing</field>
            <field name="sequence">20</field>
            <field name="fold">False</field>
            <field name="is_closed">False</field>
        </record>

        <!-- Completed Stage -->
        <record id="fieldservice_stage_completed" model="fieldservice.stage">
            <field name="name">Completed</field>
            <field name="sequence">30</field>
            <field name="fold">True</field>
            <field name="is_closed">True</field>
        </record>

        <!-- Cancelled Stage -->
        <record id="fieldservice_stage_cancelled" model="fieldservice.stage">
            <field name="name">Cancelled</field>
            <field name="sequence">40</field>
            <field name="fold">True</field>
            <field name="is_closed">True</field>
        </record>
    </data>
</odoo>
